<script>
$(document).ready(function(){
function rearrange_socialicon() {
$('#at-expanded-menu-top-service-list-container li').each(function(){
var $li = $(this);
if ($li.hasClass("at3winsvc_linkedin top-service"))
{
$( '.at3winsvc_linkedin' ).addClass( "pr0" );
}
else if ($li.hasClass("at3winsvc_facebook top-service"))
{
$( '.at3winsvc_facebook' ).addClass( "pr1" );
}
else if ($li.hasClass("at3winsvc_twitter top-service"))
{
$( '.at3winsvc_twitter' ).addClass( "pr2" );
}
else if ($li.hasClass("at3winsvc_print top-service"))
{
$( '.at3winsvc_print' ).addClass( "pr3" );
}
else if ($li.hasClass("at3winsvc_email top-service"))
{
$( '.at3winsvc_email' ).addClass( "pr4" );
}
else if ($li.hasClass("at3winsvc_gmail top-service"))
{
$( '.at3winsvc_gmail' ).addClass( "pr5" );
}
else if ($li.hasClass("at3winsvc_google_plusone_share top-service"))
{
$( '.at3winsvc_google_plusone_share' ).addClass( "pr6" );
}
else if ($li.hasClass("at3winsvc_pinterest_share top-service"))
{
$( '.at3winsvc_pinterest_share' ).addClass( "pr7" );
}
else if ($li.hasClass("at3winsvc_mailto top-service"))
{
$( '.at3winsvc_mailto' ).addClass( "pr8" );
}
else if ($li.hasClass("at3winsvc_tumblr top-service"))
{
$( '.at3winsvc_tumblr' ).addClass( "pr9" );
}
});
$(function() {
var order = $('li.top-service').sort(function(a,b) {
var classA = $(a).attr('class').replace(/^.*(pr\d).*$/, '$1');
var classB = $(b).attr('class').replace(/^.*(pr\d).*$/, '$1');
return classB < classA;
});
if ($("ul.at-expanded-menu-service-list").hasClass("arrange")){
}
else {
$('ul.at-expanded-menu-service-list').prepend( order );
$( '.at-expanded-menu-service-list' ).addClass( "arrange" );
}
});
$('.at-expanded-menu.at-expanded-menu-round').show();
}
$('a.addthis_button_more').click(function() {
$('.at-expanded-menu.at-expanded-menu-round').hide();
setTimeout(rearrange_socialicon, 400);
});
});
</script>

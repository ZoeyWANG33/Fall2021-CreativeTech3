<script>
$(window).bind("load", function() {
var programme_name = $("input[name='programName']").val();
if(programme_name == undefined) {
if(typeof utag_defaults !== 'undefined') {
var programme_name = utag_defaults.programName;
}
}
setCookie("programName", programme_name);
});
</script>
